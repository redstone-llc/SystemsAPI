{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Mod that polishes the Hypixel Housing developer experience.",
  "authors": [
    "Wekend"
  ],
  "contact": {
    "homepage": "https://wekend.dev",
    "sources": "https://github.com/Wekendd/${id}",
    "issues": "https://github.com/Wekendd/${id}/issues"
  },
  "license": "LGPL-3.0",
  "environment": "client",
  "icon": "assets/systemapi/icon.png",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "llc.redstone.systemapi.SystemAPI"
      }
    ]
  },
  "mixins": [
    "systemapi.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader}",
    "minecraft": "${minecraft}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "yet_another_config_lib_v3": ">=${yacl}"
  }
}